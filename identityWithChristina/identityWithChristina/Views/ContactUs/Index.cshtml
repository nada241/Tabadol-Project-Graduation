	<!-- contact form -->
	<div class="contact-from-section mt-150 mb-150">
		<div class="container">
			<div class="row">
				<div class="col-lg-8 mb-5 mb-lg-0">
					<div class="form-title">
						<h2>Do You have any Questions?</h2>
						<p>If you have any question, you can cotact with us, we will response as fast as possible.</p>
					</div>
				 	<div id="form_status"></div>
					<div class="contact-form">
						<form type="POST" id="fruitkha-contact" onSubmit="return valid_datas(this);">
							<p>
								<input type="text" placeholder="Name" name="name" id="name">
								<input type="tel" placeholder="Phone" name="phone" id="phone">
							</p>
							<p>
								<input type="email" placeholder="Email" name="email" id="email" style="width:98% ;">

							</p>
							<p><textarea name="message" id="message" cols="30" rows="10" placeholder="Your Question"></textarea></p>
							<input type="hidden" name="token" value="FsWga4&f6aw" />
							<p><input type="submit" value="Submit"></p>
						</form>
					</div>
				</div>
				<div class="col-lg-4">
					<div class="contact-form-wrap">
						<div class="contact-form-box">
							<h4><i class="fas fa-map"></i> Shop Address</h4>
							<p>Online</p>
						</div>
						<div class="contact-form-box">
							<h4><i class="far fa-clock"></i> Shop Hours</h4>
							<p>24/7 everyday</p>
						</div>
						<div class="contact-form-box">
							<h4><i class="fas fa-address-book"></i> Contact</h4>
							<p>Phone: +201 128 650 945 <br> Email: Tabadol@gmail.com</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
